    color: white; 
    padding-left: 8px; 
	min-height:30px;
	border-radius:0px;

   
}  
QTreeWidget::item{  
	min-height:30px;
	padding-left: 0px; 
 	color: white; 
	 border: none;
	 border-radius:0px;
	 outline:none;
}
QTreeWidget::item:hover
{
	outline:none;
	border: none;
	border-radius:0px;
	background-color:rgb(110,110,110);
 }
QTreeWidget::item:selected {  
    
