*{
	color: white;
	
}
QPushButton {  
    border: 1px solid rgb(63,63,63); 
	border-bottom-color: rgb(58,58,58);
    border-radius: 4px;  
    background-color: rgb(55,55,55); 
	color:white;

}  

QPushButton:hover
{
	
	background-color:rgb(60,60,60);
 }

QPushButton:pressed {  
    background-color: rgb(50, 50, 50); 
	border-bottom-color:rgb(58, 58, 58);
	color:rgb(208,208,208);
}
/*QPushButton[highlight="true"]*/
QPushButton[highlight="true"]{
	color: white;
	background-color: rgb(0,103,192);
	border:1px solid rgb(20,115,197);
	border-bottom: 2px solid rgb(0,62,115);
}
QPushButton[highlight="true"]:disabled{
	background-color: rgb(3, 41, 74);
}
QPushButton[highlight="true"]:hover{
	color: white;
	background-color: rgb(25,117,197);
	border:1px solid rgb(43,128,202);
	border-bottom: 2px solid rgb(15,70,118);
}
QPushButton[highlight="true"]:pressed{
	color: rgb(194,218,239);
	background-color: rgb(25,117,197);
	border: 0px ;
}

